        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:views="clr-namespace:Routindo.Plugins.WinServices.UI.Views;assembly=Routindo.Plugins.WinServices.UI"

        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TabControl>
            <TabItem Header="Watcher">
                <views:WinServiceWatcherConfigurator />
            </TabItem>
            <TabItem Header="Action">
                <views:WinServiceActionConfigurator />
            </TabItem>
            
        </TabControl>
    </Grid>
</Window>
